{
  "name": "ts-cli-boilerplate",
  "version": "0.2.2",
  "description": "TypeScript CLI boilerplate",
  "author": "Lycolia",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Lycolia/ts-cli-boilerplate"
  },
  "bin": {
    "your_command_name_here": "./dist/src/index.js"
  },
  "main": "./dist/src/index.js",
  "scripts": {
    "start": "ts-node ./src/index.ts",
    "dev": "node -r ts-node/register --inspect-brk ./src/index.ts",
    "lint": "tsc --noEmit && eslint --fix-dry-run",
    "build": "tsc",
    "test": "jest"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "commander": "^7.0.0",
    "prompts": "^2.3.2",
    "validate-npm-package-name": "^3.0.0"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/commander": "^2.12.2",
    "@types/jest": "^26.0.20",
    "@types/node": "^13.9.0",
    "@types/prompts": "^2.0.9",
    "@types/validate-npm-package-name": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "jest": "^26.6.3",
    "prettier": "^2.1.2",
    "ts-jest": "^26.5.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
